<?xml version="1.0" encoding="utf-8"?>

<WebScripts>

	<WebScript>
		<url>https://www.google.es/</url>
		
		<action>
			<checking check="title">
				<!-- The checking is done before perform the concrete action -->
				
				<value>Google</value>
			</checking>
			
			<do method="write">
				<position type="xpath">/html/body/div/div[3]/form/div[2]/div/div[1]/div/div[1]/input</position>
				<value>Halo 3</value>
			</do>
		</action>
		
		<action>
			<do method="click">
				<position type="xpath">/html/body/div/div[3]/form/div[2]/div/div[3]/center/input[1]</position>
			</do>
		</action>
		
		<action>
			<checking check="title">
				<value>Halo 3 - Buscar con Google</value>
			</checking>
		</action>
	</WebScript>
	
	<WebScript>
		<url>NonExistentURL.com</url>
	</WebScript>
	
	<WebScript>
		<url>https://www.google.es</url>
	</WebScript>
	
	<WebScript>
		<url>https://biblioteca.ua.es/es/biblioteca-de-la-universidad-de-alicante.html</url>
		
		<action>
			<checking check="title">
				<value>Biblioteca de la Universidad de Alicante. Biblioteca Universitaria</value>
			</checking>
			
			<do method="click">
				<position type="xpath">/html/body/div[3]/div[4]/div/div[2]/p[4]/a[1]</position>
			</do>
		</action>
		
		<action>
			<checking check="title">
				<value>iLink Universidad de Alicante</value>
			</checking>
		</action>
	</WebScript>
	
</WebScripts>